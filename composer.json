{
	"name" : "econgress/yii2-language-picker",
	"description" : "eCongress Yii2 Language Picker Widget based on lajax LanguagePicker",
	"type" : "yii2-extension",
	"license" : "BSD-3-Clause",
	"authors" : [{
			"name" : "econgress",
			"email" : "admin@econgress.cl"
		}
	],
	"autoload" : {
		"psr-4" : {
			"econgress\\languagepicker\\" : ""
		}
	},
	"require" : {
		"yiisoft/yii2" : "~2.0.0"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer" : "^2.11"
	},
	"scripts" : {
		"cs-fix" : "php-cs-fixer fix --config=.php_cs",
		"cs-check" : "php-cs-fixer fix --config=.php_cs --dry-run --diff"
	}
}